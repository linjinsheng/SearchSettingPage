  padding-bottom: 2px; 
  text-align: center;
} 

.selected{
  font-size: 17px;
  color:#227CD7;
  border-bottom: 3px solid #227CD7;
}


.leftProLogo{
    float:left;
    width: 70px;
    height: 70px;
    padding: 5px;
    position:relative; 
}

.leftProLogo image{
    width: 100%;
