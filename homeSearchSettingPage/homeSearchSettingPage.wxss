.mainPage {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1000;
    background: rgba(0,0,0,0.5);
    transform: translateY(100%);
    opacity: 0.5;
    transition: opacity 250ms;
    visibility: hidden;
}

.mainPage.active {
    transform: translateY(0);
    opacity: 1;
    visibility: visible;
}

