.infoSearchBar {
  position: flex;
  padding: 8px 10px;
  display: flex;
  box-sizing: border-box;
  background-color: #EFEFF4;
  border-top: 1rpx solid #D7D6DC;
  border-bottom: 1rpx solid #D7D6DC;
}

.infoSearchIcon {
  position: absolute;
  left: 40%;
  top: 5px;
}
.infoSearchBar_inBox {
  position: absolute;
  left: 10px;
  top: 7px;
}

.infoSearchBar_text {
  display: inline-block;
  font-size: 14px;
  height: 28px;
  line-height: 28px;
}

.infoSearchBar_form {
  position: relative;
  -webkit-box-flex: 1;
  -webkit-flex: auto;
  flex: auto;
  border-radius: 5px;
  background: #FFFFFF;
  border: 1rpx solid #E6E6EA;
}

.infoSearchBar_box {
  position: relative;
  padding-left: 30px;
  padding-right: 30px;
  width: 100%;
  box-sizing: border-box;
  z-index: 1;
}

.infoSearchBar_input {
  height: 28px;
  line-height: 28px;
  font-size: 14px;
}

.weui-icon-clear {
  position: absolute;
  top: 0;
  right: 0;
  padding: 7px 8px;
  font-size: 0;
}

.infoSearchBar_label {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 2;
  border-radius: 3px;
  text-align: center;
  color: #9B9B9B;
  background: #FFFFFF;
  line-height: 28px;
}

.infoSearchBar_cancelBtn {
  margin-left: 10px;
  line-height: 28px;
  color: #09BB07;
  white-space: nowrap;
}

.mainSearchPage{
  margin-top:10px;
}

.searchTitle{
	margin-left:10px;
  font-size: 16px;
  color: #888888;
}

.searchStrings{
	margin-top:5px;
}
.searchString-item{
  word-break:keep-all;   
  white-space:nowrap;  
  text-overflow: ellipsis; 
  padding:5px 15px;
  border:1px solid #d2d2d2;
  border-radius:5px;
  background-color: #d9d9d9;
  overflow: hidden; 
  margin:5px 10px;
  line-height:120%;
  font-size: 18px;
  color:#fff;
  text-align: center;
  font-family:Arial, Helvetica, sans-serif;
  width: 35%;
  display: inline-block; 
}

.searchLoadingBox {
  margin-top: 10px;
  border-top:1rpx solid #d9d9d9;
  border-left:1rpx solid #d9d9d9;
  padding-bottom: 20px;
  overflow:hidden;
}

.searchContent {
  position:relative;
  vertical-align: middle;
  float:left;
  padding:20px 10px;
  width:50%;
  box-sizing:border-box;
  border-right:1rpx solid #d9d9d9;
  border-bottom:1rpx solid #d9d9d9;
  height: 60px;
  font-size: 16px;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

/*content*/
.content{
  margin-top:40px;
}

/*资讯*/
.newsInfo{
  padding: 8px 8px;
  height: auto;
  border-bottom: 1px solid #eee;
}

/*无图片*/
.withoutPic{
    float:left;
    width: 98%;
    height: 70px;
    position:relative;
}

.left{
    float:left;
    width: 67%;
    height: 80px;
    position:relative;
}

.title{
    font-size:15px;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2; 
    overflow: hidden;
}

.right{
    float:right;
    width: 30%;
}

.right image{
    width: 100%;
    height: 80px;
    display: block;
}

/*新闻来源*/
.newsSource{
  position:absolute;
  left: 0;
  bottom: 0;
}

.newsSource text{
  color:#227CD7;
  padding-right : 5px;
  font-size : 12px;
}

/*创建时间*/
.createTime{
  position:absolute;
  right: 0;
  bottom: 0;
}

.createTime text{
    color:#999999;
    font-size : 12px;
}

.newsInfo::after{
    content:"";
    clear:both;
    position:absolute;
    height:0;
}

.newsInfo:last-child{
    border-bottom: none;
}

.tabBar{
  width: 100%;
  height: 50px;
  /* top:40px; */
  position: flex;
  opacity: 0.8;
}
 
 .tab-title{
  font-size: 16px;
   margin-left: 5%;
  /* margin-right: 10%;  */
